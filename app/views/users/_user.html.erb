<div id="<%= dom_id user %>">
  <p>
    <strong>Name:</strong>
    <%= user.name %>
  </p>

  <p>
    <strong>Gender:</strong>
    <%= user.gender %>
  </p>

  <p>
    <strong>Location:</strong>
    <%= user.location %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>Birthday:</strong>
    <%= user.birthday %>
  </p>

  <p>
    <strong>Registered:</strong>
    <%= user.registered %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= user.phone %>
  </p>

  <p>
    <strong>Cellphone:</strong>
    <%= user.cellphone %>
  </p>

  <p>
    <strong>Picture:</strong>
    <%= user.picture %>
  </p>

  <p>
    <strong>Naturalization:</strong>
    <%= user.naturalization %>
  </p>

</div>
