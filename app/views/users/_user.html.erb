<div id="<%= dom_id user %>">
  <p>
    <strong>Name:</strong>
    <%= user.name %>
  </p>

  <p>
    <strong>Gender:</strong>
    <%= user.gender %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>Picture:</strong>
    <%= user.picture %>
  </p>

  <p>
    <strong>Naturalization:</strong>
    <%= user.naturalization %>
  </p>

</div>
