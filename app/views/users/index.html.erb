<p style="color: green"><%= notice %></p>

<h1>Users</h1>

<%= button_to "Get some users", api_v1_users_path, method: :post %>

<div id="users">
  <% @users.each do |user| %>
    <%= render user %>
    <p>
      <%= link_to "Show this user", user %>
    </p>
  <% end %>
</div>

<%= link_to "New user", new_user_path %>
